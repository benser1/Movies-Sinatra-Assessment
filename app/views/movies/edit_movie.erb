<h1>Edit Movie</h1>


<form method="POST" action="/movies/<%= @movie.id %>/edit">
  <textarea type="text" name="movie[name]"><%= @movie.name %></textarea>
  <textarea type="text" name="movie[year_released]"><%= @movie.year_released %></textarea>
  <button type="submit">Edit Movie</button>
</form>


